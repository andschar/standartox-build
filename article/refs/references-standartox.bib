@article{clements_community_2009,
  title = {{{COMMUNITY RESPONSES TO CONTAMINANTS}}: {{USING BASIC ECOLOGICAL PRINCIPLES TO PREDICT ECOTOXICOLOGICAL EFFECTS}}},
  shorttitle = {{{COMMUNITY RESPONSES TO CONTAMINANTS}}},
  author = {Clements, William H. and Rohr, Jason R.},
