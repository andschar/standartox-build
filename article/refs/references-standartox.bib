  author = {Riley, William D. and Potter, Edward C.E. and Biggs, Jeremy and Collins, Adrian L. and Jarvie, Helen P. and Jones, J. Iwan and {Kelly-Quinn}, Mary and Ormerod, Steve J. and Sear, David A. and Wilby, Robert L. and Broadmeadow, Samantha and Brown, Colin D. and Chanin, Paul and Copp, Gordon H. and Cowx, Ian G. and Grogan, Adam and Hornby, Duncan D. and Huggett, Duncan and Kelly, Martyn G. and Naura, Marc and Newman, Jonathan R. and Siriwardena, Gavin M.},
  year = {2018},
